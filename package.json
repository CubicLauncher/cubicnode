{
  "name": "cubic-neutron",
  "version": "1.0.0",
  "description": "Un módulo que permite la descarga y la ejecución de minecraft java con node.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "minecraft-launcher",
    "minecraft-core",
    "minecraft-launcher-core",
    "optifine",
    "forge",
    "fabric"
  ],
  "license": "MIT",
  "author": "Santiagolxx",
  "main": "dist/index.js",
  "homepage": "https://github.com/CubicLauncher/neutron",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CubicLauncher/neutron"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "bundle:types": "rollup -c rollup.dts.config.mjs",
    "build": "npm run build:types && npm run bundle:types && npm run build",
    "test": "jest \"lib/.*\\.test\\.js\" --env=node",
    "lint": "eslint src"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.15.17",
    "adm-zip": "^0.5.16",
    "axios": "^1.9.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "oxlint": "^0.16.10",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "concurrently": "^9.1.2",
    "rollup": "^4.40.2",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "^5.8.3"
  },
  "trustedDependencies": [
    "core-js"
  ]
}
