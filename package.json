{
  "name": "cubic-neutron",
  "version": "1.0.0",
  "description": "Un módulo que permite la descarga y la ejecución de minecraft java con node.js",
  "types": "dist/handler.d.ts",
  "type": "module",
  "keywords": [
    "minecraft-launcher",
    "minecraft-core",
    "minecraft-launcher-core",
    "optifine",
    "forge",
    "fabric"
  ],
  "license": "MIT",
  "author": "Santiagolxx",
  "main": "./index",
  "homepage": "https://github.com/CubicLauncher/neutron",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CubicLauncher/neutron"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "concurrently -n types,js \"npm run build:types\" \"npm run build:js\"",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps",
    "test": "jest \"lib/.*\\.test\\.js\" --env=node",
    "lint": "eslint src"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.15.17",
    "adm-zip": "^0.5.16",
    "axios": "^1.9.0",
    "oxlint": "^0.16.10",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "typescript": "^5.8.3"
  },
  "trustedDependencies": [
    "core-js"
  ]
}
